/**
 * 4-way demultiplexor:
 * [a, b, c, d] = [in, 0, 0, 0] if sel = 00
 *                [0, in, 0, 0] if sel = 01
 *                [0, 0, in, 0] if sel = 10
 *                [0, 0, 0, in] if sel = 11

 * [a, b] = [in, 0] if sel = 0
 *          [0, in] if sel = 1
 * [c, d] = [in, 0] if sel = 0
 *          [0, in] if sel = 1
 */
CHIP DMux4Way {
    IN in, sel[2];
    OUT a, b, c, d;

    PARTS:
    Not(in=sel[1], out=ns1);
    And(a=ns1, b=in, out=ab); //get the in
    And(a=sel[1], b=in, out=cd);

    DMux(in=ab, sel=sel[0], a=a, b=b);
    DMux(in=cd, sel=sel[0], a=c, b=d);
}